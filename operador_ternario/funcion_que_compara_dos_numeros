function compararNumeros(a, b){
  return (a === b)
  ? "a y b son iguales"
  :(a > b)
  ? "a es mayor que b"
  : "b es mayor que a"
};

console.log(compararNumeros(11, 27)); //b es mayor que a
console.log(compararNumeros(27, 11)); //a es mayor que b
console.log(compararNumeros(15, 15)); //a y b son iguales

